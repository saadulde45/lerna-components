{
  "name": "wc-nested-react-component",
  "version": "0.1.4",
  "description": "Nested react component",
  "author": "Saad Ulde",
  "homepage": "",
  "license": "MIT",
  "main": "dist/wc-nested-react-component.js",
  "directories": {
    "src": "src",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build-old": "npm run build:react && npm run build:bundle",
    "build:react": "react-scripts build",
    "build:bundle": "webpack --config webpack.config.js",
    "common": "$(npm bin)/mkdirp dist && $(npm bin)/copyfiles index.html dist && $(npm bin)/copyfiles -a -u 1 'src/**/*.scss' dist",
    "build": "npm run common && babel src --out-dir dist",
    "transpile": "babel src -d dist --ignore '**/*.spec.js,**/*.stories.js'",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://localhost:8081/repository/npm-private/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-jest": "24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "classnames": "^2.2.6",
    "copyfiles": "^2.1.0",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-plugin-import": "^2.18.2",
    "file-loader": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "24.7.1",
    "node-sass": "^7.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2",
    "webpack-node-externals": "^1.7.2"
  }
}
