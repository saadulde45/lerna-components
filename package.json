{
  "name": "lerna-components",
  "version": "0.1.0",
  "devDependencies": {
    "lerna": "^3.15.0",
    "node-sass": "^4.12.0",
    "react-scripts": "^3.0.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "scripts": {
    "publish": "lerna version && lerna publish from-package --no-verify-access --registry=http://localhost:8081/repository/npm-private/",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "config": {
    "unsafe-perm": true
  },
  "publishConfig": {
    "registry": "http://localhost:8081/repository/npm-private/"
  },
  "repository": {
    "url": "https://github.com/saadulde45/lerna-components/",
    "type": "git"
  },
  "author": "Saad Ulde",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
